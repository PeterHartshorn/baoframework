{
  "name": "bao-framework",
  "version": "1.0.13",
  "description": "A lightweight framework from Thailand",
  "main": "dist/Bao.js",
  "types": "dist/Bao.d.ts",
  "scripts": {
    "build": "npm-run-all lint compile",
    "lint": "tslint src/**/*ts",
    "compile": "mkdir -p dist && tsc -t ES3 --baseUrl dist --outDir dist `find src -name *.ts`",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Switch-Media/baoframework.git"
  },
  "bin": {
    "baopack": "./tools/packer"
  },
  "keywords": [
    "framework",
    "typescript",
    "javascript",
    "bao"
  ],
  "devDependencies": {
    "browserify": "^14.4.0",
    "stringify": "^5.1.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "rimraf": "^2.6.1",
    "npm-run-all": "^4.0.2"
  },
  "author": "Bao.Bao@asia.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Switch-Media/baoframework/issues"
  },
  "homepage": "https://github.com/Switch-Media/baoframework#readme"
}
